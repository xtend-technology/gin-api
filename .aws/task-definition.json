{
    "family": "gin-simple-server",
    "containerDefinitions": [
        {
            "name": "gin-simple-server",
            "image": "444973598085.dkr.ecr.us-east-2.amazonaws.com/go-api:latest",
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 8080,
                    "protocol": "tcp"
                }
            ],
            "essential": true
        }
    ],
    "requiresCompatibilities": [
        "EC2"
    ],
    "cpu": "150",
    "memory": "300"
}